<?xml version="1.0"?>

<launch>
  <!-- ========================== Additional Launch Files ========================= -->
  <include file="$(find fastrack_crazyflie_demos)/launch/rviz.launch"/>
  <include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch" >
     <arg name="port" value="9090"/>
  </include>

  <group ns="drone1">
  	<include file="$(find userpath)/launch/sw_demo.launch">
  		<arg name="prefix" value="drone1" />
      <arg name="init_y" value="0.0" />
  	</include>
  </group>

  <group ns="drone2">
  	<include file="$(find userpath)/launch/sw_demo.launch">
  		<arg name="prefix" value="drone2" />
      <arg name="init_y" value="1.0" />
  	</include>
  </group>


  <include file="$(find userpoint_publisher)/launch/publisher.launch"> </include> 
</launch>